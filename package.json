  },
  "devDependencies": {
    "@types/esprima": "^4.0.2",
    "@types/jest": "^27.4.1",
    "@types/node": "^15.3.0",
    "@types/yargs": "^16.0.1",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.6.0",
    "typescript": "^4.6.2",
